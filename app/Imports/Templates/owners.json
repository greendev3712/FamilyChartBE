{
    "importerClass": "App\\Imports\\Importers\\OwnerImporter",
    "entryLimit": 5000,
    "stopsOnIssues": false,
    "sheets": [{
        "name": "owners",
        "columns": [{
            "name": "name",
            "laravelValidations": "string|required|unique:owners,name",
            "complexValidations": "unique_in_column"
        }, {
            "name": "description",
            "laravelValidations": "string|nullable"
        }, {
            "name": "is_active",
            "laravelValidations": "required|boolean"
        }]
    }]
}