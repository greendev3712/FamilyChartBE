{
    "name": "laravel-enso/enso",
    "description": "Laravel Enso can be a solid start for any Laravel Based project",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "laravel-enso/addressesmanager": "2.1.*",
        "laravel-enso/commentsmanager": "2.1.*",
        "laravel-enso/contacts": "2.1.*",
        "laravel-enso/dataimport": "2.1.*",
        "laravel-enso/documentsmanager": "2.1.*",
        "laravel-enso/notifications": "2.1.*",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "laravel-enso/testhelper": "1.4.*",
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --force --tag='enso-assets'"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
