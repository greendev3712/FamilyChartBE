{
    "name": "laravel-enso/enso",
    "description": "Laravel Enso can be a solid start for any Laravel Based project",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-enso/addressesmanager": "2.2.*",
        "laravel-enso/commentsmanager": "2.2.*",
        "laravel-enso/contacts": "2.1.*",
        "laravel-enso/core": "2.5.*",
        "laravel-enso/charts": "2.2.*",
        "laravel-enso/dataimport": "2.2.*",
        "laravel-enso/documentsmanager": "2.2.*",
        "laravel-enso/notifications": "2.1.*",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.2",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "^0.8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "codedungeon/phpunit-result-printer": "^0.13.0",
        "filp/whoops": "^2.1.0",
        "fzaninotto/faker": "^1.7.0",
        "laracasts/generators": "^1.1",
        "laravel-enso/testhelper": "1.4.*",
        "mockery/mockery": "1.*",
        "nunomaduro/collision": "^2.0.0",
        "phpunit/phpunit": "^7.0",
        "wnx/laravel-stats": "^1.6"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --force --tag='enso-assets'",
            "php artisan vendor:publish --tag='localisation-lang-files'",
            "php artisan localisation:merge"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
